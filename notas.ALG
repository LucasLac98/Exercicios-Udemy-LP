Algoritmo "notas"

Var

nota1 , nota2 , notaTotal : real


Inicio

  escreva("Digite a primeira nota:")
   leia(nota1)
   
  escreva("Digite a segunda nota:")
   leia(nota2)
   
   notaTotal <- nota1 + nota2
  escreval("A nota final do aluno é",notaTotal:8:1)
   
   se notaTotal < 60 então
     escreval("REPROVADO")
     
   fimse


Fimalgoritmo